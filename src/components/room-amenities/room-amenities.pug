mixin room-amenities(data)
  .room-amenities 
    .room-amenities__item-list 
      each item in data
        .room-amenities__item 
          .room-amenities__container-icon
            span(class=`room-amenities__icon ${item.icon}`)
          .room-amenities__container-texts
            span.room-amenities__title #{item.title}
            span.room-amenities__text #{item.text}